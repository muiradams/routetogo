{
  "name": "routetogo",
  "version": "1.0.0",
  "description": "A single-page web app for finding all possible flight routes between two cities",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "postinstall": "webpack -p --config ./webpack.config.prod.js",
